
   <div id="content" class="app-content" role="main">
  		<div class="app-content-body "> 
			<div class="wrapper-md">
				<div class="panel panel-default">
			    	<div class="panel-heading font-bold">                  
			      		Numbers
			    	</div>
			    	<div class="panel-body">
			    	    <div style="margin-bottom:10px">
			      			<a href="/contectNumbers" class="btn btn-info">Buy Number</a>
			      		</div>
			      		<% if(data.error!= '' && data.error!=undefined){ %>
			      		<div class="wrapper text-center">
                          <strong ><%= data.error %></strong>
                         </div>
			    		<div>
			    		<% } %>
			      		<table  class="table" >
								<thead>
								
									<tr>
										<th>Sr.No.</th>
										<th>Contact Name</th>
										<th>Phone Number</th>
										<th>Action</th>									
										<th>Time Allocation</th>
									</tr>
								</thead>
								<tbody>
									<%  var i=1; numbers. forEach(function(number) { %>
									<tr>
								    <td><%= i %></td>
								    <td><%= number.contact_name %></td>
								    <td><%= number.phone_number %></td>
										<td><a href="/numbers/music/<%= number.id %>" class="btn btn-sm btn-primary">Music</a>
										<a href="/numbers/delete/<%= number.id %>" onclick="return confirm('Are you sure you want to delete this number?');" class="btn btn-sm btn-danger">Release Number</a>
										<a href="/allocateNumberToUSer/number/<%= number.id %>" class="btn btn-sm btn-primary">Aloocate User</a>
										</td>

										<td><a href="/allocateTime/number/<%= number.id %>" class="btn btn-sm btn-info">Time Allocation</a></td>
									</tr>
									<% i++ }); %>
								</tbody>
							</table>
			      		</div>
			    	</div>

			  	</div>
			</div>
		</div>
  	</div> 
  <!-- /content -->
  
  <!-- footer -->

  <!-- / footer
