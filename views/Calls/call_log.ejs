<div id="content" class="app-content" role="main">
  		<div class="app-content-body "> 
			<div class="wrapper-md">
				<div class="panel panel-default">
			    	<div class="panel-heading font-bold">                  
			      		Call Log
			    	</div>
			    	<div class="panel-body">	
			    	<div style="margin-bottom:10px">
			      			<a href="/twilio" class="btn btn-info">New Call</a>
			      		</div>		    		
			      		<div>
			      			<table  class="table" >
								<thead>
									<tr>
										<th>Sr.No.</th>
										<th>From</th>
										<th>To</th>
										<th>CallType</th>
										<th>Date</th>
										<th>start_time</th>
										<th>end_time</th>
										<th>duration</th>
										<th>status</th>
										<th>Recording</th>

									</tr>
								</thead>
								<tbody>
									<%  var i=1; data.forEach(function(dataob) { %>
								 	<tr>
										<td><%= i++ %></td>
										<td><%= dataob.from %></td>
										<td><%= dataob.to %></td>
										<td><%= dataob.callType %></td>
										<td><%= dataob.date %></td>
										<td><%= dataob.from_time %></td>
										<td><%= dataob.to_time %></td>
										<td><%= dataob.duration %></td>
										<td><%= dataob.status %></td>
										<td><% if(dataob.recordingURL != '') { %><audio controls><source src="<%= dataob.recordingURL %>.mp3" type="audio/mpeg"></audio><% } else {} %></td>
									 <!--    <td><%= dataob.sid %></td>
										<td><%= moment.tz(dataob.start_time,"Asia/Kolkata").format('DD-MM-YYYY HH:mm') %></td>
										<td><%= moment.tz(dataob.end_time, "Asia/Kolkata").format('DD-MM-YYYY HH:mm') %></td>										 
									
										<td><%= dataob.duration %></td>
										<td><%= dataob.status %></td> -->
									   									
									</tr> 
									<% }); %>
								
								</tbody>
							</table>
			      		</div>
			    	</div>

			  	</div>
			</div>
		</div>
  	</div> 
  